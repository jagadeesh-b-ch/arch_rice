{
  "group/controls": {
    "orientation": "inherit",
    "modules": ["wireplumber", "backlight"],
  },
  "wireplumber": {
    "format": "{volume}% {icon}",
    "format-muted": "",
    "on-click": "hyprctl dispatch -- exec [tag +bar_launch] pwvucontrol",
    "format-icons": ["", "", ""],
  },
  "backlight": {
    "device": "intel_backlight",
    "format": "{percent}% {icon}",
    "format-icons": ["", "", "", "", "", "", "", "", ""],
  },
}
